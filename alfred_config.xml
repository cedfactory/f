<?xml version="1.0"?>
<dings>
    <ding export="alfred">
        <import filename="./tiase/data/test/google_stocks_data.csv" export="input.csv"/>
        <!--<features indicators="simple_rtn,rsi_30,williams_%r" target="target" export="indicators.csv"/>-->
        <features target="target" indicators="" export="indicators.csv"/>
        <!--<preprocessing export="preprocessing.csv">
            <outliers method="outliers_normalize_stdcutoff" />
            <transformation method="log" indicators="rsi_30" />
            <transformation method="x2" indicators="williams_%r" />
            <discretization indicator="rsi_30" method="unsupervised" nbins="2" />
        </preprocessing>
        <featureengineering export="featureengineering.csv">
            <reduction method="correlation" threshold="0.95" />
        </featureengineering>-->
        <classifiers>
            <classifier id="decision_tree_3" type="decision tree" export="decision_tree_3" >
                <parameters>
                    <parameter name="max_depth" value="3"/>
                </parameters>
            </classifier>
            <classifier id="decision_tree_5" type="decision tree" export="decision_tree_5" >
                <parameters>
                    <parameter name="max_depth" value="5"/>
                </parameters>
            </classifier>
            <classifier id="decision_tree_7" type="decision tree" export="decision_tree_7" >
                <parameters>
                    <parameter name="max_depth" value="7"/>
                </parameters>
            </classifier>
            <classifier id="svc_1" type="svc" />
            <classifier id="svc_1" type="svc">
                <parameters>
                    <parameter name="kernel" value="poly"/>
                </parameters>
            </classifier>
            <classifier id="lstm1_50" type="lstm1" export="lstm1_50">
                <parameters>
                    <parameter name="epochs" value="50"/>
                </parameters>
            </classifier>
            <classifier id="lstm2_50" type="lstm2" export="lstm2_50">
                <parameters>
                    <parameter name="epochs" value="50"/>
                </parameters>
            </classifier>
            <classifier id="lstm3_50" type="lstm3" export="lstm3_50">
                <parameters>
                    <parameter name="epochs" value="50"/>
                </parameters>
            </classifier>
            <classifier id="grid_search_1" type="grid search">
                <parameters>
                    <parameter name="classifier" value="decision_tree_3"/>
                </parameters>
            </classifier>
            <classifier id="voting_1" type="voting">
                <parameters>
                    <parameter name="classifiers" value="decision_tree_3,decision_tree_5,svc_1,lstm1_50,lstm2_50"/>
                </parameters>
            </classifier>
        </classifiers>
    </ding>
</dings>