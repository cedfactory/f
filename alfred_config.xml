<?xml version="1.0"?>
<dings>
    <ding>
        <import value="AI.PA"/>
        <features indicators="simple_rtn,rsi_30,williams_%r" target="target"/>
        <preprocessing>
            <outliers method="outliers_stdcutoff" />
            <transformations>
                <transformation method="log" indicators="rsi_30" />
                <transformation method="x2" indicators="williams_%r" />
            </transformations>
            <discretizations>
                <discretization indicator="rsi_30" method="usupervised" nbins="2" />
            </discretizations>
        </preprocessing>
        <featureengeineering>
            <reduction method="correlation" threshold="0.95" />
        </featureengeineering>
        <classifier name="svc"/>
    </ding>
</dings>