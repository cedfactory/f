<?xml version="1.0"?>
<dings>
    <ding>
        <import value="AI.PA"/>
        <features indicators="simple_rtn,rsi_30,williams_%r" target="target"/>
        <preprocessing>
            <outliers method="outliers_normalize_stdcutoff" />
            <transformations>
                <transformation method="log" indicators="rsi_30" />
                <transformation method="x2" indicators="williams_%r" />
            </transformations>
            <discretizations>
                <discretization indicator="rsi_30" method="unsupervised" nbins="2" />
            </discretizations>
        </preprocessing>
        <featureengeineering>
            <reduction method="correlation" threshold="0.95" />
        </featureengeineering>
        <export filename="./tmp/df.csv"/>
        <classifiers>
            <classifier id="decision_tree_1" type="decision tree" export="./tmp/decision_tree_1" />
            <classifier id="svc_1" type="svc" />
            <classifier id="lstm1_1" type="lstm1" export="./tmp/lstm1_1">
                <parameters>
                    <parameter name="epochs" value="15"/>
                </parameters>
            </classifier>
            <classifier id="grid_search_1" type="grid search">
                <parameters>
                    <parameter name="classifier" value="decision_tree_1"/>
                </parameters>
            </classifier>
            <classifier id="voting_1" type="voting">
                <parameters>
                    <parameter name="classifiers" value="decision_tree_1,svc_1,lstm1_1"/>
                </parameters>
            </classifier>   
        </classifiers>
    </ding>
</dings>